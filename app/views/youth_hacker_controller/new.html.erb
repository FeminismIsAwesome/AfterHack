<div class="containerYouthPage">
  
<%= form_for @youth_hacker, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <div >
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
 <div >
  <%= f.label :age %>
  <%= f.text_field :age %>
  </div>
  <div >
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  </div>
  <div>
  <%= f.label :email_address %>
  <%= f.text_field :address %>
  </div>
  <div>
  <%= f.label :category %>
  <%= f.text_field :category %>
  </div>
 <div >
  <%= f.label :project_name %>
  <%= f.text_field :projectname %>
  </div>
  <%= f.submit "Create" %>
<% end %>

</div>
  
